{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "src/root.html"
      },
      {
        "src": "/login.html",
        "dest": "/src/Login/login.html"
      },
      {
        "src": "/exp.html",
        "dest": "/public/exp.html"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      }
    ]
  }