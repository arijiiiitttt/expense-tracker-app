{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "/root.html"
      },
      {
        "src": "/",
        "dest": "/src/login/log.html"
      },
      {
        "src": "/app",
        "dest": "/public/exp.html"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }