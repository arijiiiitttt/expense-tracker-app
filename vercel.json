{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "/root.html"
      },
      {
        "src": "/login",
        "dest": "/src/login.html"
      },
      {
        "src": "/exp",
        "dest": "/public/exp.html",
        "headers": {
          "Cache-Control": "no-store"
        }
      },
      {
        "src": "/script/script.js",
        "dest": "/src/script/script.js"
      },
      {
        "src": "/img/.*",
        "dest": "/src/img/$1"
      },
      {
        "src": "/public/exp.html",
        "status": 404
      }
    ]
  }